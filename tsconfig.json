{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
        "module": "esNext",
        "moduleResolution": "node",
        "target": "esNext",
        "sourceMap": true,
        //apparently these next two are needed for lodash as there is no default export
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true
    }
}
